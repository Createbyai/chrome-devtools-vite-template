{
  "name": "chrome-devtools-panel-template",
  "version": "1.0.1",
  "description": "Vue 3 + Vite template for building Chrome DevTools panel extensions",
  "type": "module",
  "files": [
    "template/**",
    "index.js"
  ],
  "main": "index.js",
  "bin": {
    "create-chrome-devtools-panel": "./bin/cli.js"
  },
  "keywords": [
    "chrome-extension",
    "devtools",
    "vue",
    "vite",
    "template"
  ],
  "scripts": {
    "build": "vite build",
    "watch": "vite build --watch",
    "build:extension": "vite build && cd dist && zip -r chrome-devtools-extension.zip ."
  },
  "author": "Createbyai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Createbyai/chrome-devtools-panel-template"
  },
  "bugs": {
    "url": "https://github.com/Createbyai/chrome-devtools-panel-template/issues"
  },
  "homepage": "https://github.com/Createbyai/chrome-devtools-panel-template#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "vite": "^5.0.10"
  }
}
